{
  "name": "fastify-boiler",
  "version": "1.0.0",
  "description": "fastify boiler plate for first trial",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint ."
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaranin-b/fastify-boilerplate.git"
  },
  "keywords": [
    "fastify"
  ],
  "author": "Jaranin Boonpee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaranin-b/fastify-boilerplate/issues"
  },
  "homepage": "https://github.com/jaranin-b/fastify-boilerplate#readme",
  "dependencies": {
    "fastify": "^3.15.1",
    "fastify-env": "^2.1.1",
    "fastify-plugin": "^3.0.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "jest": "^27.0.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "sinon": "^11.1.1",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
